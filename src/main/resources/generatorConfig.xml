<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE
        generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="MySql">
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/sas_database?useUnicode=true&amp;characterEncoding=utf-8&amp;useSSL=false"
                        userId="root" password="Cmb@2025"/>
        <javaModelGenerator targetPackage="com.sas.entity" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="com.sas.mapper" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!-- Mapper接口生成器 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.sas.mapper" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        <table tableName="assets" domainObjectName="assetEntity"/>
        <table tableName="companys" domainObjectName="companyEntity"/>
        <table tableName="laoe" domainObjectName="laoeEntity"/>
        <table tableName="profit" domainObjectName="profitEntity"/>
    </context>
</generatorConfiguration>

